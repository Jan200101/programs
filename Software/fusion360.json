    "Parameters": {
        "dxvk": true,
        "discrete_gpu": true,
        "sync": "esync"
    },
    "Steps": [
        {
            "action": "run_script",
            "script": "cd !bottle_drive/windows/system32/\ncp icinfo.exe ie4uinit.exe\n"
        },
        {
            "action": "install_exe",
            "file_name": "Fusion 360 Admin Install.exe",
            "url": "https://dl.appstreaming.autodesk.com/production/installers/Fusion%20360%20Admin%20Install.exe",
            "rename": "Fusion360installer.exe",
            "file_checksum": "cf2713e186448328f0add0371135f053",
            "arguments": "-p deploy -g -f log.txt --quiet"
        }
    ]
}